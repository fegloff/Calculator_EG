<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <include android:id="@+id/toolbar" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Hola Don Pepito"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_marginStart="8dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <Button
        android:id="@+id/button_mc"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:text="mc"
        app:layout_constraintEnd_toStartOf="@+id/button_mplus"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_mplus"
        android:text="m+"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_msub"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_mc"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_msub"
        android:text="m-"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_mr"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_mplus"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_mr"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="mr"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_msub"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_c"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="C"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_div"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_mc"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_div"
        android:text="/"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_mul"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_c"
        app:layout_constraintTop_toBottomOf="@+id/button_mplus"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_mul"
        android:text="x"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_del"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_div"
        app:layout_constraintTop_toBottomOf="@+id/button_msub"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_del"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="del"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_mul"
        app:layout_constraintTop_toBottomOf="@+id/button_mr"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_7"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="7"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_8"
        app:layout_constraintEnd_toStartOf="@+id/button_8"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_c"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_8"
        android:text="8"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_9"
        app:layout_constraintEnd_toStartOf="@+id/button_9"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_7"
        app:layout_constraintTop_toBottomOf="@+id/button_div"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_9"
        android:text="9"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_subs"
        app:layout_constraintEnd_toStartOf="@+id/button_subs"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_8"
        app:layout_constraintTop_toBottomOf="@+id/button_mul"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_subs"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="-"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_9"
        app:layout_constraintTop_toBottomOf="@+id/button_del"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_4"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="4"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_5"
        app:layout_constraintEnd_toStartOf="@+id/button_5"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_7"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_5"
        android:text="5"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_6"
        app:layout_constraintEnd_toStartOf="@+id/button_6"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_4"
        app:layout_constraintTop_toBottomOf="@+id/button_8"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_6"
        android:text="6"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_plus"
        app:layout_constraintEnd_toStartOf="@+id/button_plus"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_5"
        app:layout_constraintTop_toBottomOf="@+id/button_9"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_plus"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="+"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_6"
        app:layout_constraintTop_toBottomOf="@+id/button_subs"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_1"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="1"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_2"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_4"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_2"
        android:text="2"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_3"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_1"
        app:layout_constraintTop_toBottomOf="@+id/button_5"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_3"
        android:text="3"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_equal"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_2"
        app:layout_constraintTop_toBottomOf="@+id/button_6"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_equal"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="="
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".26"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_3"
        app:layout_constraintTop_toBottomOf="@+id/button_plus"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_percent"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="%"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_1"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_0"
        android:text="0"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_decimal"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_percent"
        app:layout_constraintTop_toBottomOf="@+id/button_2"
        app:layout_constraintWidth_percent=".21" />

    <Button
        android:id="@+id/button_decimal"
        android:text="."
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@+id/button_equal"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_0"
        app:layout_constraintTop_toBottomOf="@+id/button_3"
        app:layout_constraintWidth_percent=".21" />
</android.support.constraint.ConstraintLayout>